{
    "name": "akimmaksimov85/creator",
    "description": "Clean Architecture Code Creator",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "nette/php-generator": "*",
        "symfony/console": "6.1.*",
        "symfony/framework-bundle": "6.1.*",
        "symfony/messenger": "6/.1.*",
        "symfony/validator": "6.1.*",
        "ramsey/uuid-doctrine": "^1.6",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "2.7.*",
        "doctrine/orm": "^2.8"
